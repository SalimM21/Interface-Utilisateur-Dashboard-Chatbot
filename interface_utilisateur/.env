# Variables d'environnement pour l'Interface Utilisateur

# URL de l'API Gateway

API_URL=[http://localhost:8000](http://localhost:8000)

# URL de Keycloak pour l'authentification

KEYCLOAK_URL=[http://localhost:8080](http://localhost:8080)
KEYCLOAK_REALM=InterfaceUtilisateur
KEYCLOAK_CLIENT_ID=dashboard-client
KEYCLOAK_CLIENT_SECRET=<votre_secret>

# Configuration de la base de données

MONGO_URI=mongodb://localhost:27017/interface_utilisateur

# Configuration Rasa Chatbot

RASA_URL=[http://localhost:5005](http://localhost:5005)
RASA_TOKEN=<votre_token>

# Variables de monitoring

PROMETHEUS_URL=[http://localhost:9090](http://localhost:9090)
GRAFANA_URL=[http://localhost:3001](http://localhost:3001)
LOKI_URL=[http://localhost:3100](http://localhost:3100)

# Paramètres généraux

ENV=development
DEBUG=True

# Ports

DASHBOARD_PORT=3000
CHATBOT_PORT=5005
API_GATEWAY_PORT=8000
